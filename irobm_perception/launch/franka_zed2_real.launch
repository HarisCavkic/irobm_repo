<launch>
    <!-- Include Franka Control Launch File -->
    <include file="$(find franka_control)/launch/franka_control.launch"/>

    <!-- Include Camera -->
    <include file="$(find zed_wrapper)/launch/franka_control.launch"/>

    <!-- Static Transform Publisher for Camera -->
    <node pkg="tf" type="static_transform_publisher" name="camera_link_broadcaster"
          args="-0.097397 0 0.0274111 0 -0.824473 0 panda_hand zed2_left_camera" />


</launch>